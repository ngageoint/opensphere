{
  "build": {
    "config": "config/settings.json",
    "type": "app",
    "index": "index.js",
    "scss": "scss/example.scss",
    "gcc": {
      "define": [
        "os.SETTINGS=config/settings.json",
        "os.config.appNs='example'"
      ],
      "entry_point": [
        "goog:example"
      ],
      "hide_warnings_for": [
        "/opensphere/"
      ]
    },
    "moduleDefines": {
      "my.module.PATH": "my-module/dist/my-module.min.js"
    }
  }
}
