    "projections": [
      {
        "code": "EPSG:3031",
        "extent": [
          -12369000,
          -12369000,
          12369000,
          12369000
        ],
        "isGlobal": false,
        "proj4": "+proj=stere +lat_0=-90 +lat_ts=-71 +lon_0=0 +k=1 +x_0=0 +y_0=0 +datum=WGS84 +units=m +no_defs",
